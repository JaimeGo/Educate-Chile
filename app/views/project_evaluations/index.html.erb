<p id="notice"><%= notice %></p>

<h1>Project Evaluations</h1>

<table>
  <thead>
    <tr>
      <th>Questions</th>
      <th>Answers</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_evaluations.each do |project_evaluation| %>
      <tr>
        <td><%= project_evaluation.questions %></td>
        <td><%= project_evaluation.answers %></td>
        <td><%= link_to 'Show', project_evaluation %></td>
        <td><%= link_to 'Edit', edit_project_evaluation_path(project_evaluation) %></td>
        <td><%= link_to 'Destroy', project_evaluation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project Evaluation', new_project_evaluation_path %>
